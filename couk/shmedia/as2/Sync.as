class couk.shmedia.as2.Sync extends couk.shmedia.as2.Logger {		private var _id:String;	private var _conn:LocalConnection;	private var _callback:Function;		public function Sync(id:String, callback:Function) {		_id = id;		_conn = new LocalConnection();		_conn.connect(_id);		_conn.callback = callback;	}		public function send(val:Object):Void {		_conn.send(_id, "callback", val);	}	}